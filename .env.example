# Supabase 설정
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# 앱 설정
NEXT_PUBLIC_APP_NAME="달래마켓"
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# 타임존 설정
TZ=Asia/Seoul

# Gemini API 설정
NEXT_PUBLIC_GEMINI_API_KEY=your_gemini_api_key

# Cloudinary 설정
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret
NEXT_PUBLIC_CLOUDINARY_UPLOAD_PRESET=your_upload_preset

# YouTube Data API v3 설정
YOUTUBE_API_KEY=your_youtube_api_key

# 네이버 클라우드 SENS SMS 인증 설정
NCP_ACCESS_KEY=your_ncp_access_key
NCP_SECRET_KEY=your_ncp_secret_key
NCP_SERVICE_ID=your_ncp_service_id
NCP_SENDER_PHONE=your_sender_phone

# 네이버 로그인 OAuth 설정
NEXT_PUBLIC_NAVER_CLIENT_ID=your_naver_client_id
NAVER_CLIENT_SECRET=your_naver_client_secret
NEXT_PUBLIC_NAVER_CALLBACK_URL=http://localhost:3000/auth/callback/naver

# JWT Secret for admin impersonate
JWT_SECRET=your_jwt_secret_key

# 네이버 데이터랩 API 설정 (검색 트렌드 분석)
NAVER_DATALAB_CLIENT_ID=your_datalab_client_id
NAVER_DATALAB_CLIENT_SECRET=your_datalab_client_secret

# 네이버 쇼핑 검색 API 설정 (경쟁사 가격 모니터링)
NAVER_CLIENT_ID=your_shopping_client_id
NAVER_CLIENT_SECRET=your_shopping_client_secret

# OneSignal 푸시 알림 설정
NEXT_PUBLIC_ONESIGNAL_APP_ID=your_onesignal_app_id
ONESIGNAL_REST_API_KEY=your_onesignal_rest_api_key

# Resend 이메일 발송 설정
RESEND_API_KEY=your_resend_api_key
FROM_EMAIL=onboarding@resend.dev
FROM_NAME=달래마켓
EMAIL_TEST_MODE=true
TEST_EMAIL=your_test_email@example.com

# 로컬 개발용 배포 정보 (Vercel에서는 자동 설정됨)
NEXT_PUBLIC_VERCEL_GIT_COMMIT_SHA=local_development